<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.wangchenhui.model.User">
	<insert id="add" parameterType="cn.wangchenhui.model.User">
		insert into user values(null,#{user_name},#{user_pass},#{user_gender},#{birthday},#{address},#{telphone},#{email},#{type},#{status},#{is_operator},#{opt_status},#{is_admin})
	</insert>
</mapper>